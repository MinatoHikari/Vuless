$color-default = rgba(0, 0, 0, 0.65)
$color-primary = #41b883
$color-flamingo = #ff5722
$color-border = rgba(0, 0, 0, 0.12)

$font-size-title = 18px
$vl-padding-md = 20px

$colors = primary flamingo
getColor(color)
    if color == primary
        return $color-primary
    else if color == flamingo
        return $color-flamingo
